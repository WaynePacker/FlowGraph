                                        <Path.Data>
                                            <PathGeometry Figures="M 8.5098772 1.0628502 V 15.851168" FillRule="NonZero"/>
                                        </Path.Data>
                                    </Path>
                                </Canvas>
                            </Canvas>
                        </Canvas>
                    </Viewbox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

